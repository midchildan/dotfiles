{
  "article template": {
    "prefix": "article",
    "body": [
      "\\documentclass[a4paper, 12pt]{article}",
      "\\usepackage{biblatex}",
      "\\usepackage{booktabs}",
      "\\usepackage{graphicx}",
      "\\usepackage{here}",
      "\\usepackage{hyperref}",
      "\\usepackage{listings}",
      "\\usepackage{longtable}",
      "\\usepackage{xcolor}",
      "\\addbibresource{${TM_FILENAME_BASE}.bib}",
